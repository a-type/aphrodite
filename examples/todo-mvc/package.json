{
  "name": "@aphro/todo-mvc",
  "version": "1.0.5",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "serve": "pnpm ts-build && pnpm copy && webpack serve",
    "build": "pnpm ts-build && pnpm copy  && webpack",
    "aphro": "aphro gen ./src/domain.aphro --dest ./src/generated && cp src/generated/*.sql dist/generated",
    "ts-clean": "tsc --build --clean",
    "ts-build": "tsc --build",
    "ts-watch": "tsc --build -w",
    "copy": "cp src/index.html dist/index.html && cp src/generated/*.sql dist/generated"
  },
  "dependencies": {
    "@aphro/absurd-sql-connector": "workspace:*",
    "@aphro/react": "workspace:*",
    "@aphro/runtime-ts": "workspace:*",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@aphro/codegen-cli": "workspace:*",
    "@types/react": "^18.0.10",
    "html-webpack-plugin": "^5.3.2",
    "raw-loader": "^4.0.2",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
