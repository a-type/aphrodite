engine: sqlite
db: todomvc

User as Node {
  id: ID<User>
  name: NaturalLanguage
  created: Timestamp
  modified: Timestamp
} & OutboundEdges {
  todos: Edge<Todo.ownerId>
} & Mutations {
  create {
    name
  }
}

Todo as Node {
  id: ID<Node>
  text: NaturalLanguage
  created: Timestamp
  modified: Timestamp
  ownerId: ID<User>
} & Mutations {
  create {
    text
  }
}